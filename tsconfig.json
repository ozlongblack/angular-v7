{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "core": [
        "app/core/index.ts"
      ],
      "core/*": [
        "app/core/*"
      ],
      "environments": [
        "environments"
      ],
      "material": [
        "app/material/index.ts"
      ],
      "models": [
        "app/models/index.ts"
      ],
      "shared": [
        "app/shared/index.ts"
      ],
      "shared/*": [
        "app/shared/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
